echo '' >> fix_orb_sym.out.all
echo $1 >> fix_orb_sym.out.all
executable="pyscf/shciscf/Lz_relabel_f90/fix_orb_sym"
cmd="$2pyscf/shciscf/Lz_relabel_f90/fix_orb_sym < $1 > fix_orb_sym.out"
eval $cmd
grep 'ORBSYM=' fix_orb_sym.out > ORBSYM
grep 'num_switch.*=' fix_orb_sym.out >> ORBSYM
ex $file <<_end
1r ORBSYM
2s/ * /,/g
2s/ORBSYM=,/ORBSYM=    /
3,30s/ORBSYM/ORBSYM_old/
g/num_switch/d
g/ORBSYM_old=/d
wq
_end
cat fix_orb_sym.out >>fix_orb_sym.out.all
